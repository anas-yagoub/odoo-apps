<odoo>
    <data>
          	<!-- View inheritance for Partner Model-->
        <record id="inherited_account_move_form" model="ir.ui.view">
            <field name="name">Inherited form View for account move</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='asset_category_id']" position="attributes">
              		<attribute name="invisible">1</attribute>
                </xpath>
                 <xpath expr="//field[@name='discount']" position="attributes">
              		<attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='tax_ids']" position="attributes">
              		<attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='analytic_tag_ids']" position="attributes">
              		<attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='ware_id']" position="before">
                    <field name="analytic_account_id"/>
                </xpath>

            </field>
        </record>
         	<!-- View inheritance for Partner Model-->
        <record id="inherited_account_move_list" model="ir.ui.view">
            <field name="name">Inherited list View for account move</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_invoice_tree"/>
            <field name="arch" type="xml">
                 <xpath expr="//tree" position="attributes">
                     <attribute name="decoration-danger">(invoice_date_due &gt;= current_date)and(invoice_payment_state != 'paid')</attribute>
                 </xpath>
<!--                <xpath expr="//tree" position="attributes">-->
<!--                     <attribute name="decoration-primary">(invoice_date_due >= current_date)and(invoice_payment_state != 'paid')</attribute>-->
<!--                </xpath>-->
<!--                <xpath expr="//tree" position="attributes">-->
<!--                     <attribute name="decoration-primary">(invoice_date_due &lt; current_date)and(invoice_payment_state != 'paid')</attribute>-->
<!--                </xpath>-->
                <xpath expr="//field[@name='invoice_origin']" position="attributes">
              		<attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='amount_untaxed_signed']" position="attributes">
              		<attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='invoice_date']" position="before">
                    <field name="ware_id"/>
                </xpath>
                <xpath expr="//field[@name='invoice_date_due']" position="after">
                    <field name="amount_total"/>
                </xpath>
            </field>
        </record>
        <record model="ir.ui.view" id="inventory_form_view">
            <field name="name">account_inventory</field>
            <field name="model">account.inventory</field>
            <field name="arch" type="xml">
                <form string="Inventory Form">
                   <group>
                       <group>
                           <field name="name_scq"/>
                           <field name="name"/>
                       </group>
                   </group>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="inventory_tree_view">
            <field name="name">account_inventory_tree</field>
            <field name="model">account.inventory</field>
            <field name="arch" type="xml">
                <tree string="Inventory List">
                    <field name="name_scq"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="inventory_list_action">
            <field name="name">inventory</field>
            <field name="res_model">account.inventory</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Inventory" id="inventory_m_pro" parent="account.menu_finance_receivables"
              action="inventory_list_action" sequence="111"/>
        <menuitem name="Analytic Account" id="analtic_account" parent="account.menu_finance"
              action="analytic.action_account_analytic_account_form"/>
        <menuitem name="Analytic Items" id="analtic_items_account" parent="account.menu_finance"
              action="analytic.account_analytic_line_action_entries"/>
    </data>
</odoo>